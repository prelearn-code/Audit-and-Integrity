{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build:insert_test",
      "type": "shell",
      "command": "/usr/bin/g++",
      "args": [
        "-fdiagnostics-color=always",
        "-std=c++17",
        "-g",
        "-O0",
        "${workspaceFolder}/system_test/insert_files/insert_test.cpp",
        "${workspaceFolder}/Storage-node/storage_node.cpp",
        "CLIENT_CPP_PATH",
        "-I",
        "${workspaceFolder}/Storage-node",
        "-I",
        "${workspaceFolder}/system_test/insert_files",
        "-I",
        "/usr/local/include",
        "-I",
        "/usr/include/jsoncpp",
        "-L",
        "/usr/local/lib",
        "-o",
        "${workspaceFolder}/bin/insert_test",
        "-lpbc",
        "-lgmp",
        "-ljsoncpp",
        "-lcrypto",
        "-lssl",
        "-lpthread"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": ["$gcc"]
    },
