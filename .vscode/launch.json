{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug insert_test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/system_test/insert_files/insert_test",
      "args": [
        "--out",
        "${workspaceFolder}/out/insert",
        "--data-dir",
        "${workspaceFolder}/node_data",
        "--param",
        "${workspaceFolder}/data/insert.json",
        "--enc",
        "${workspaceFolder}/data/test.enc"
      ],
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build:insert_test",
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "stopAtEntry": false,
      "externalConsole": false,
      "setupCommands": [
        {
          "description": "Enable pretty printing",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
